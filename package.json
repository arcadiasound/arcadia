{
  "name": "arcadia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "pnpm clean-build && node scripts/add-asset-prefix.mjs",
    "postbuild": "node scripts/remove-asset-prefix.mjs && prettier --write './next.config.js'",
    "build": "pnpm prebuild && next build && next export && pnpm postbuild",
    "upload": "pnpm build && pnpm irys",
    "irys": "irys upload-dir out -h https://node2.irys.xyz --wallet /Users/Shared/wallet1.json -t arweave --index-file index.html --tags App-Name Arcadia --no-confirmation",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write '**/*.{json,md,mjs,js,ts,jsx,tsx,yml,css}'",
    "clean": "pnpm clean-build && rm -rf node_modules",
    "clean-build": "rm -rf out*"
  },
  "dependencies": {
    "@ariakit/react": "0.3.5",
    "@next/font": "13.1.6",
    "@permaweb/stampjs": "0.6.0",
    "@radix-ui/react-accordion": "1.1.2",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-hover-card": "1.0.7",
    "@radix-ui/react-slider": "1.1.2",
    "@radix-ui/react-tabs": "1.0.4",
    "@splidejs/react-splide": "0.7.12",
    "@stitches/react": "1.2.8",
    "@tanstack/react-query": "4.28.0",
    "arconnect": "1.0.3",
    "arweave": "1.13.7",
    "arweave-account": "1.4.3",
    "arweave-graphql": "0.0.5",
    "arweave-wallet-connector": "1.0.2",
    "chart.js": "4.4.0",
    "formik": "2.4.5",
    "lodash.debounce": "4.0.8",
    "motion": "10.15.5",
    "motion-hooks": "0.1.1",
    "next": "13.1.6",
    "next-themes": "0.2.1",
    "permaweb-orderbook": "1.6.52",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "react-router-dom": "6.11.2",
    "warp-contracts": "1.4.19",
    "warp-contracts-plugin-deploy": "1.0.12",
    "warp-contracts-plugin-signature": "1.0.17"
  },
  "devDependencies": {
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "prettier": "2.8.4",
    "typescript": "4.7.4"
  }
}
